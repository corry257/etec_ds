// Criar a tabuada, onde o usuário entre com o valor e ele calcule a tabuada do valor digitado.

programa {
  funcao inicio() {
    
    inteiro num, produto, cont
    cont = 0

    enquanto (cont < 10)
    
    { escreva ("\n Entre com um número: ")
    leia (num)

    produto = num * >= 10  
    escreva(produto)
    
    }
  }
}
